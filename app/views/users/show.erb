<h1>You're Posts<h1>
<br>

  <main role="main">
  <p>Your Post List:</p>
  <ul class="channels-list no-bullets no-padding">
    <% @user.posts.each do |post| %>
      <li>
        <a href="/posts/<%= post.id %>"><%= post.title %></a>
      </li>
    <% end %>
  </ul>
  <div>
  	<h4>Total All Time Posts:<h4><%= @user.posts.all.count %>
  </div>
</main>